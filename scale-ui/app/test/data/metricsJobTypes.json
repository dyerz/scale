        },
        {
            "name": "post_time_max",
            "title": "Post-task Time (Max)",
            "description": "Maximum time spent finalizing the job task.",
            "units": "seconds",
            "group": "post_time",
            "aggregate": "max"
        },
        {
            "name": "post_time_avg",
