{
  "id": 4,
  "name": "cem",
  "title": "Constrained Energy Minimization",
  "version": "1.0",
  "description": "CEM is used for target detection in hyperspectral remote sensing imagery.",
  "author_name": "DigitalGlobe",
  "author_url": "https://en.wikipedia.org/wiki/WorldView-2",
  "category": "spectral",
  "is_operational": true,
  "icon_code": "f185",
  "docker_privileged": false,
  "docker_image": "10.4.4.10:5000/cem:1.0",
  "priority": 1,
  "timeout": 30,
  "max_tries": 3,
  "cpus_required": 4.0,
  "mem_required": 4096.0,
  "disk_out_const_required": 2048.0,
  "disk_out_mult_required": 0.0,
  "interface": {
    "output_data": [{
      "media_type": "image/tiff",
      "required": true,
      "type": "file",
      "name": "cem-detections"
    }],
    "shared_resources": [],
    "command_arguments": "${infile} ${job_output_dir}",
    "input_data": [{
      "media_types": ["image/nitf"],
      "required": true,
      "type": "file",
      "name": "image"
    },{
      "media_types": ["text/csv"],
      "required": true,
      "type": "file",
      "name": "signature"
    }],
    "version": "1.0",
    "command": "./cem.sh"
  },
  "error_mapping": {}
}