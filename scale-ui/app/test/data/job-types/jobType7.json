  "category": "landsat",
  "is_operational": true,
  "icon_code": "f00c",
  "docker_privileged": false,
  "docker_image": "10.4.4.10:5000/ls8-atmos-corr:2.1.9",
  "priority": 1,
  "timeout": 30,
  "max_tries": 3,
